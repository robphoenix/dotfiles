{
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "mdx",
    "markdown.mdx",
    "graphql"
  ],
  "tsserver.formatOnType": false,
  "tsserver.enableJavascript": false,
  "coc.preferences.formatOnType": false,
  "prettier.tabWidth": 2,
  "emmet.includeLanguages": {
    "htmldjango": "html"
  },
  "snippets.loadFromExtensions": true,
  "htmldjango.formatting.provider": "djlint",
  "htmldjango.djlint.indent": 2
}
